{
  "name": "nodeauth",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lint": "eslint ."
  },
  "dependencies": {
    "autoprefixer": "*",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.15.1",
    "connect-flash": "*",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-messages": "*",
    "express-session": "^1.14.0",
    "express-validator": "*",
    "jade": "~1.11.0",
    "jquery": "*",
    "material-design-lite": "*",
    "mongodb": "*",
    "mongoose": "*",
    "moment": "*",
    "monk": "*",
    "morgan": "~1.7.0",
    "multer": "*",
    "node-sass-middleware": "*",
    "passport": "*",
    "passport-http": "*",
    "passport-local": "*",
    "postcss-middleware": "*",
    "postcss-safe-parser": "*",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "eslint": "*",
    "eslint-config-google": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": "google",
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "mocha": true,
      "mongo": true
    },
    "rules": {
      "require-jsdoc": 0,
      "quote-props": [
        2,
        "as-needed"
      ],
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ],
      "no-console": 2,
      "no-unused-vars": [
        2,
        {
          "vars": "local",
          "args": "after-used"
        }
      ]
    }
  }
}
